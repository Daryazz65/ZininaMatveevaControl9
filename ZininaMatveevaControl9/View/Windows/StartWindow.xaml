<Window x:Class="ZininaMatveevaControl9.View.Windows.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZininaMatveevaControl9.View.Windows"
        mc:Ignorable="d"
        Title="StartWindow" Height="570" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".5*"/>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <StackPanel Background="Yellow">
            <Border Height="100"/>
            <TextBlock FontSize="20" Margin="10 0 0 0">
                <Hyperlink Click="Hyperlink_Click">Добавить группу</Hyperlink>
            </TextBlock>
            <Border Height="20"/>
            <TextBlock FontSize="20" Margin="10 0 0 0">
                <Hyperlink Click="Hyperlink_Click_1">Добавить соревнование</Hyperlink>
            </TextBlock>
            <Border Height="20"/>
            <TextBlock FontSize="20" Margin="10 0 0 0">
                <Hyperlink Click="Hyperlink_Click_2">Учёт</Hyperlink>
            </TextBlock>
        </StackPanel>

        <Frame Name="MainFrm" Grid.Column="1"/>

    </Grid>
</Window>
